<div class="center-cell-wrapper">
    <div class="details center-cell">
        <span class="name">{{ user.profile.fullName() }}</span>
        <span class="email">{{ user.username }}</span>
    </div>
</div>
<div class="picture left-cell">
    <img src="{{ user.profile.avatarPath('small') }}" alt="your picture"/>
</div>
<div class="more right-cell">
    <a><span>more</span></a>
</div>
<ul class="dropdown-content right">
</ul>
